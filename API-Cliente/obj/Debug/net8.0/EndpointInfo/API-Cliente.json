            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreaClienteDto": {
        "required": [
          "apellidos",
          "nombre"
        ],
        "type": "object",
        "properties": {
          "nombre": {
            "minLength": 1,
            "type": "string"
          },
          "apellidos": {
            "minLength": 1,
            "type": "string"
          },
          "correo": {
            "pattern": "^[a-zA-Z0-9_\\.-]+@([a-zA-Z0-9-]+\\.)+[a-zA-Z]{2,6}$",
            "type": "string",
            "nullable": true
          },
          "telefono": {
            "type": "string",
